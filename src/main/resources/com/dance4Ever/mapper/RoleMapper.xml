<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dance4Ever.mapper.RoleMapper">
	<select id="selectByRole" resultType="Role" parameterType="String">
		select * from role
		where role = #{role}
	</select>
</mapper>